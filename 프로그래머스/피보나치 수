//재귀로 풀었을 때 - 시간초과 & 런타임에러
function fibonacci (n) {
   if(n<=2) return 1;
    return fibonacci(n-2) + fibonacci(n-1);
}

function solution(n) {
    return fibonacci(ㅁn);
}


//반복문으로 해결
function solution(n) {
    const fibonacci = [0, 1];
    for(let i = 2; i<=n; i++){
        fibonacci[i] = (fibonacci[i-1]+ fibonacci[i-2])%1234567
    }
    
    return fibonacci[n];
}
